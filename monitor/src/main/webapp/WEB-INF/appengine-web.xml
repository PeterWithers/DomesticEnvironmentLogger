<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <version>1</version>
  <threadsafe>true</threadsafe>
  <runtime>java8</runtime>
  <automatic_scaling>
    <max_idle_instances>1</max_idle_instances>
    <min_pending_latency>automatic</min_pending_latency>
    <max_pending_latency>30ms</max_pending_latency>
  </automatic_scaling>
  <instance-class>F1</instance-class>
</appengine-web-app>
